<div class="main">
      <div class="jumbotron m-auto text-center" style="width:70%; background:transparent">
          <h1>Recipes</h1>
          <div></div>
      </div>
      {{#each data}}
      <div class="card mx-auto my-3" style="width:70%">
          <div class="card-header">
              <h3 class="card-title">{{data.ttile}}</h3>
          </div>
          <div class="card-body">
              <div class="row">
                  <div class="col-md-4">
                    <img src="{{data.url}}">
                  </div>
                  <div class="col-md-5">
                      <h5>Nutrition Profile (per serving):</h5>
                    <ul>
                        <li>calories: {{data.calories}}</li>
                        <li>protein: {{data.protein}}g</li>
                        <li>carbs: {{data.carbs}}g</li>
                        <li>fats: {{data.fats}}g</li>
                    </ul>
                  </div>
                  <div class="col-md-3">
                      <h6>Cook Time: 1 hour</h6>
                      <h6>Servings: {{data.serves}}</h6>
                      <a href="{{data.url}}">recipe link</a>
                    </div>
              </div>
          </div>
      </div>
      {{/each}}

  </div>

